<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	id="WebApp_ID" version="3.0">
	<display-name>Projet annuel</display-name>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.htm</welcome-file>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>default.html</welcome-file>
		<welcome-file>default.htm</welcome-file>
		<welcome-file>default.jsp</welcome-file>
	</welcome-file-list>
	<servlet>
		<servlet-name>XMLGen</servlet-name>
		<servlet-class>com.project.servlets.XMLGen</servlet-class>
		<init-param>
			<param-name>auteur</param-name>
			<param-value>BigTeam</param-value>
		</init-param>
	</servlet>
	<servlet>
		<servlet-name>XSLGen</servlet-name>
		<servlet-class>com.project.servlets.XSLGen</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>FileUpload</servlet-name>
		<servlet-class>com.project.servlets.FileUpload</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Navigation</servlet-name>
		<servlet-class>com.project.servlets.Navigation</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Contact</servlet-name>
		<servlet-class>com.project.servlets.Contact</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>FormViewer</servlet-name>
		<servlet-class>com.project.servlets.FormViewer</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>CreateView</servlet-name>
		<servlet-class>com.project.servlets.CreateView</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>XMLGen</servlet-name>
		<url-pattern>/XMLgen</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>XSLGen</servlet-name>
		<url-pattern>/XSLgen</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>FileUpload</servlet-name>
		<url-pattern>/Fileupload</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Navigation</servlet-name>
		<url-pattern>/Navig</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Contact</servlet-name>
		<url-pattern>/contact</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>FormViewer</servlet-name>
		<url-pattern>/Formviewer</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>CreateView</servlet-name>
		<url-pattern>/CreateView</url-pattern>
	</servlet-mapping>
</web-app>